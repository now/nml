<grammar>

  <!-- quoted string literals -->
  <terminal name="literal">
    <delimited>
      <begin>'</begin>
      <end>'</end>
    </delimited>
    <delimited>
      <begin>"</begin>
      <end>"</end>
    </delimited>
  </terminal>

  <!-- node test -->
  <terminal name="processingInstruction">
    <delimited>
      <begin>processing-instruction(</begin>
      <end>)</end>
    </delimited>
  </terminal>

  <terminal name="end">
    <end/>
  </terminal>

  <ignore char=" "/>

  <terminal xmlns:t="http://www.pingdynasty.com/namespaces/tokenizer" xmlns:p="http://www.pingdynasty.com/namespaces/parser" name="ncname"><any>1234567890_abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ#-</any></terminal><terminal xmlns:t="http://www.pingdynasty.com/namespaces/tokenizer" xmlns:p="http://www.pingdynasty.com/namespaces/parser" name="slash"><equals>/</equals></terminal><terminal xmlns:t="http://www.pingdynasty.com/namespaces/tokenizer" xmlns:p="http://www.pingdynasty.com/namespaces/parser" name="slashslash"><equals>//</equals></terminal><terminal xmlns:t="http://www.pingdynasty.com/namespaces/tokenizer" xmlns:p="http://www.pingdynasty.com/namespaces/parser" name="dot"><equals>.</equals></terminal><terminal xmlns:t="http://www.pingdynasty.com/namespaces/tokenizer" xmlns:p="http://www.pingdynasty.com/namespaces/parser" name="dotdot"><equals>..</equals></terminal><terminal xmlns:t="http://www.pingdynasty.com/namespaces/tokenizer" xmlns:p="http://www.pingdynasty.com/namespaces/parser" name="at"><equals>@</equals></terminal><terminal xmlns:t="http://www.pingdynasty.com/namespaces/tokenizer" xmlns:p="http://www.pingdynasty.com/namespaces/parser" name="colon"><equals>:</equals></terminal><terminal xmlns:t="http://www.pingdynasty.com/namespaces/tokenizer" xmlns:p="http://www.pingdynasty.com/namespaces/parser" name="lparen"><equals>(</equals></terminal><terminal xmlns:t="http://www.pingdynasty.com/namespaces/tokenizer" xmlns:p="http://www.pingdynasty.com/namespaces/parser" name="rparen"><equals>)</equals></terminal><terminal xmlns:t="http://www.pingdynasty.com/namespaces/tokenizer" xmlns:p="http://www.pingdynasty.com/namespaces/parser" name="lbrack"><equals>[</equals></terminal><terminal xmlns:t="http://www.pingdynasty.com/namespaces/tokenizer" xmlns:p="http://www.pingdynasty.com/namespaces/parser" name="rbrack"><equals>]</equals></terminal><terminal xmlns:t="http://www.pingdynasty.com/namespaces/tokenizer" xmlns:p="http://www.pingdynasty.com/namespaces/parser" name="asterisk"><equals>*</equals></terminal><terminal xmlns:t="http://www.pingdynasty.com/namespaces/tokenizer" xmlns:p="http://www.pingdynasty.com/namespaces/parser" name="or"><equals>or</equals></terminal><terminal xmlns:t="http://www.pingdynasty.com/namespaces/tokenizer" xmlns:p="http://www.pingdynasty.com/namespaces/parser" name="and"><equals>and</equals></terminal><terminal xmlns:t="http://www.pingdynasty.com/namespaces/tokenizer" xmlns:p="http://www.pingdynasty.com/namespaces/parser" name="div"><equals>div</equals></terminal><terminal xmlns:t="http://www.pingdynasty.com/namespaces/tokenizer" xmlns:p="http://www.pingdynasty.com/namespaces/parser" name="mod"><equals>mod</equals></terminal><terminal xmlns:t="http://www.pingdynasty.com/namespaces/tokenizer" xmlns:p="http://www.pingdynasty.com/namespaces/parser" name="minus"><equals>-</equals></terminal><terminal xmlns:t="http://www.pingdynasty.com/namespaces/tokenizer" xmlns:p="http://www.pingdynasty.com/namespaces/parser" name="plus"><equals>+</equals></terminal><terminal xmlns:t="http://www.pingdynasty.com/namespaces/tokenizer" xmlns:p="http://www.pingdynasty.com/namespaces/parser" name="equalityOperator"><equals>=</equals><equals>!=</equals></terminal><terminal xmlns:t="http://www.pingdynasty.com/namespaces/tokenizer" xmlns:p="http://www.pingdynasty.com/namespaces/parser" name="relationalOperator"><equals>&lt;</equals><equals>&lt;=</equals><equals>&gt;</equals><equals>&gt;=</equals></terminal><terminal xmlns:t="http://www.pingdynasty.com/namespaces/tokenizer" xmlns:p="http://www.pingdynasty.com/namespaces/parser" name="axisName"><equals>ancestor::</equals><equals>ancestor-or-self::</equals><equals>attribute::</equals><equals>child::</equals><equals>descendant::</equals><equals>descendant-or-self::</equals><equals>following::</equals><equals>following-sibling::</equals><equals>namespace::</equals><equals>parent::</equals><equals>preceding::</equals><equals>preceding-sibling::</equals><equals>self::</equals></terminal><terminal xmlns:t="http://www.pingdynasty.com/namespaces/tokenizer" xmlns:p="http://www.pingdynasty.com/namespaces/parser" name="comment"><equals>comment()</equals></terminal><terminal xmlns:t="http://www.pingdynasty.com/namespaces/tokenizer" xmlns:p="http://www.pingdynasty.com/namespaces/parser" name="text"><equals>text()</equals></terminal><terminal xmlns:t="http://www.pingdynasty.com/namespaces/tokenizer" xmlns:p="http://www.pingdynasty.com/namespaces/parser" name="node"><equals>node()</equals></terminal><construct xmlns:t="http://www.pingdynasty.com/namespaces/tokenizer" xmlns:p="http://www.pingdynasty.com/namespaces/parser" name="Expr"><option><part name="LocationPath"/><part name="end"/></option></construct><construct xmlns:t="http://www.pingdynasty.com/namespaces/tokenizer" xmlns:p="http://www.pingdynasty.com/namespaces/parser" name="LocationPath"><option><part name="RelativeLocationPath"/></option><option><part name="AbsoluteLocationPath"/></option></construct><construct xmlns:t="http://www.pingdynasty.com/namespaces/tokenizer" xmlns:p="http://www.pingdynasty.com/namespaces/parser" name="AbsoluteLocationPath"><option><part name="slash"/><part name="RelativeLocationPath"/></option><option><part name="slashslash"/><part name="RelativeLocationPath"/></option><option><part name="slash"/></option></construct><construct name="RelativeLocationPath"><option><part name="Step"/><part name="RelativeLocationPath-rest"/></option></construct><construct name="RelativeLocationPath-rest"><option><part name="slash"/><part name="Step"/><part name="RelativeLocationPath-rest"/></option><option><part name="slashslash"/><part name="Step"/><part name="RelativeLocationPath-rest"/></option><option><!-- empty --></option></construct><construct xmlns:t="http://www.pingdynasty.com/namespaces/tokenizer" xmlns:p="http://www.pingdynasty.com/namespaces/parser" name="Step"><option><part name="AxisSpecifier"/><part name="NodeTest"/></option><option><part name="AbbreviatedStep"/></option></construct><construct xmlns:t="http://www.pingdynasty.com/namespaces/tokenizer" xmlns:p="http://www.pingdynasty.com/namespaces/parser" name="AxisSpecifier"><option><part name="axisName"/></option><option><part name="at"/></option><option/></construct><construct xmlns:t="http://www.pingdynasty.com/namespaces/tokenizer" xmlns:p="http://www.pingdynasty.com/namespaces/parser" name="AbbreviatedStep"><option><part name="dot"/></option><option><part name="dotdot"/></option></construct><construct xmlns:t="http://www.pingdynasty.com/namespaces/tokenizer" xmlns:p="http://www.pingdynasty.com/namespaces/parser" name="NodeTest"><option><part name="NameTest"/></option><option><part name="NodeType"/></option></construct><construct xmlns:t="http://www.pingdynasty.com/namespaces/tokenizer" xmlns:p="http://www.pingdynasty.com/namespaces/parser" name="NodeType"><option><part name="node"/></option><option><part name="text"/></option><option><part name="comment"/></option><option><part name="processingInstruction"/></option></construct><construct xmlns:t="http://www.pingdynasty.com/namespaces/tokenizer" xmlns:p="http://www.pingdynasty.com/namespaces/parser" name="NameTest"><option><part name="ncname"/></option></construct>

  <import href="xpath-lexer.xsl"/>

  <import href="xpath-templates.xsl"/>

</grammar>
