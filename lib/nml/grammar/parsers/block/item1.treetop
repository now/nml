module NML::Grammar::Parsers::Block
  grammar Item1
    include Blocks2
    include Inlines2

    rule item1
      first
      rest:blocks2
      <NML::Grammar::Nodes::Block::Item>
    end

    rule first
      inlines:inlines2 ("\n" / !.)
      <NML::Grammar::Nodes::Block::Paragraph>
    end
  end
end
