module NML::Grammar::Parsers::Block
  grammar Item3
    include Blocks4
    include Inlines4

    rule item3
      first:first3
      rest:blocks4
      <NML::Grammar::Nodes::Block::Item>
    end

    rule first3
      inlines:inlines4 ("\n" / !.)
      <NML::Grammar::Nodes::Block::Paragraph>
    end
  end
end
